{
    "name": "edic",
    "displayName": "EDiC",
    "description": "EDiC support for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "publisher": "Nik-Sch",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/Nik-Sch/EDiC-vscode-syntax"
    },
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "edic",
                "aliases": [
                    "EDiC",
                    "edic"
                ],
                "extensions": [
                    ".s",
                    ".S",
                    ".asm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "edic",
                "scopeName": "source.edic",
                "path": "./syntaxes/edic.tmLanguage.json"
            }
        ]
    }
}